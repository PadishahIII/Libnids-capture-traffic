include=""
libs=" -lrt -ldl  "


usage () {
        cat <<EOF
Usage: $0 [OPTIONS]
Options:
        --include        [$include]
        --libs           [$libs]
EOF
        exit 1
}

if test $# -le 0; then usage; fi

while test $# -gt 0; do
        case $1 in
        --include) echo "$include" ;;
        --libs)    echo "$libs" ;;
        *)         usage ;;
        esac

        shift
done

exit 0
